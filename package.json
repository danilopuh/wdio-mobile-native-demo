{
  "name": "wdio-mobile-native-demo",
  "version": "1.0.0",
  "private": true,
  "description": "WebdriverIO + Appium automation for WebdriverIO native-demo-app (Android & iOS) with Mocha, Chai, Allure, GitLab CI and BrowserStack integration.",
  "scripts": {
    "postinstall": "npx wdio config -y || exit 0",
    "android": "wdio wdio.android.conf.js",
    "ios": "wdio wdio.ios.conf.js",
    "bs": "wdio wdio.bs.conf.js",
    "clean:reports": "rimraf ./reports ./allure-results ./allure-report || true",
    "report:allure": "allure generate --clean allure-results && allure open",
    "wdio": "wdio run ./wdio.conf.js"
  },
  "devDependencies": {
    "@wdio/allure-reporter": "^8.36.0",
    "@wdio/appium-service": "^8.36.0",
    "@wdio/cli": "^8.36.0",
    "@wdio/local-runner": "^9.20.0",
    "@wdio/mocha-framework": "^9.20.0",
    "@wdio/spec-reporter": "^9.20.0",
    "allure-commandline": "^2.27.0",
    "appium": "^2.9.0",
    "appium-uiautomator2-driver": "^3.8.0",
    "appium-xcuitest-driver": "^5.12.0",
    "chai": "^4.4.1",
    "dotenv": "^16.4.5",
    "mocha": "^10.6.0",
    "rimraf": "^5.0.10"
  }
}
